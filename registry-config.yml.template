# registry-config.yml.template
# Configuration for Docker Distribution Registry v3 (CNCF)
version: 0.1

log:
  level: info
  formatter: text

storage:
  filesystem:
    rootdirectory: /var/lib/registry
  delete:
    enabled: true
  maintenance:
    uploadpurging:
      enabled: true
      age: 168h
      interval: 24h
      dryrun: false

http:
  addr: 0.0.0.0:5000
  secret: ${REGISTRY_HTTP_SECRET}
  headers:
    X-Content-Type-Options: [nosniff]
    X-Frame-Options: [SAMEORIGIN]